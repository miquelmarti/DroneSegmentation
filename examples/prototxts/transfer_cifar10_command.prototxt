stage {
    name: "fast_stage"
    solver_filename: "examples/cifar10/solver.prototxt"
    cmd_solver {
        command: "python examples/cifar10/solve.py --solver examples/cifar10/solver.prototxt --nb_iter 500"
    }
}
stage {
    name: "slow_stage"
    solver_filename: "examples/cifar10/solver_lr1.prototxt"
    cmd_solver {
        command: "python examples/cifar10/solve.py --solver examples/cifar10/solver_lr1.prototxt --weights examples/cifar10/cifar10_iter_500.caffemodel --nb_iter 300"
    }
}
stage {
    name: "style_stage"
    solver_filename: "examples/cifar10/solver_style.prototxt"
    cmd_solver {
        command: "python examples/cifar10/solve.py --solver examples/cifar10/solver_style.prototxt --weights examples/cifar10/cifar10_lr1_iter_300.caffemodel --nb_iter 100"
    }
    
    freeze: "conv1"
    freeze: "conv2"
    ignore: "ip2"
}
