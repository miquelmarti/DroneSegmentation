train_net: "examples/train_fcn/fcn32/train.prototxt"
test_net: "examples/train_fcn/fcn32/val.prototxt"
test_iter: 1111
# make test net, but don't invoke it from the solver itself
test_interval: 4000
display: 20
average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax
base_lr: 1e-10
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 300
weight_decay: 0.0005
snapshot: 100
snapshot_prefix: "examples/train_fcn/fcn32/fcn32"
test_initialization: false
