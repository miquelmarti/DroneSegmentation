#out_dir: "snapshots"
init_weights: "/home/shared/givenModels/vgg-16/VGG_ILSVRC_16_layers.caffemodel"

multi_source {
    init_stage {
        name: "init"
        solver_filename: "../train_fcn/fcn32/solver.prototxt"
        
        ignore: "fc6"
        ignore: "fc7"
        
        fcn_surgery: true
        halt_percentage: 40.0
    }
    
    stage {
        name: "train_fcn-32"
        solver_filename: "../train_fcn/fcn32/solver.prototxt"
        
        fcn_surgery: true
        #halt_percentage: 10.0
    }
    stage {
        name: "train_fcn-16"
        solver_filename: "../train_fcn/fcn16/solver.prototxt"
        
        fcn_surgery: true
        #halt_percentage: 10.0
    }
    stage {
        name: "train_fcn-8"
        solver_filename: "../train_fcn/fcn8/solver.prototxt"
        
        fcn_surgery: true
        #halt_percentage: 10.0
    }
}


