#out_dir: "snapshots"
init_weights: "/home/shared/givenModels/vgg-16/VGG_ILSVRC_16_layers.caffemodel"

multi_source {
    iterations: 5
    init_stage {
        name: "init"
        solver_filename: "../train_fcn/fcn32/solver.prototxt"
        
        ignore: "fc6"
        ignore: "fc7"
        
        fcn_surgery: true
        #halt_percentage: 10.0
    }
    
    stage {
        name: "ms5_fcn32"
        solver_filename: "../train_fcn/fcn32/solver.prototxt"
        
        fcn_surgery: true
    }
    stage {
        name: "ms5_fcn16"
        solver_filename: "../train_fcn/fcn16/solver.prototxt"
        
        fcn_surgery: true
        # halt_percentage: 50
    }
    stage {
        name: "ms5_fcn8"
        solver_filename: "../train_fcn/fcn8/solver.prototxt"
        
        fcn_surgery: true
        # halt_percentage: 40
    }    
}
