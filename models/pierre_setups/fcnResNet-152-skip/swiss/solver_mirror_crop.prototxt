train_net: "train_mirror_crop.prototxt"
# make test net, but don't invoke it from the solver itself
test_interval: 0
display: 100
average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax
base_lr: 1e-10
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 100000
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "snapshots/train_mirror_crop"
test_initialization: false
