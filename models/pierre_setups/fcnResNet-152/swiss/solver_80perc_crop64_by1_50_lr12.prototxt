train_net: "train_80perc_crop64_by1.prototxt"
#test_net: "val_80perc_crop64_by1.prototxt"
#test_iter: 1280
# make test net, but don't invoke it from the solver itself
test_interval: 0
display: 100
average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax
base_lr: 1e-12
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 50000
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "snapshots/80perc_crop64_by1_50_lr12"
test_initialization: false
