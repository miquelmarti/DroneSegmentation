train_net: "train_all_crop4_by2.prototxt"
#test_net: "val_all_crop4_by2.prototxt"
#test_iter: 126
# make test net, but don't invoke it from the solver itself
test_interval: 0
display: 100
average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax
base_lr: 1e-10
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 50000
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "snapshots/all_crop4_by2_50"
test_initialization: false
